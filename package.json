{
  "name": "Aiba-Arena2",
  "version": "0.0.1",
  "scripts": {
    "bp": "blueprint",
    "start": "blueprint run",
    "build:contracts": "blueprint build AibaJetton && blueprint build AibaJettonSupply && blueprint build ArenaVault && blueprint build AibaToken && blueprint build ArenaRewardVault && blueprint build BrokerNFT && blueprint build BrokerMarketplaceEscrow && blueprint build AiAssetRegistry && blueprint build AiAssetMarketplaceEscrow && blueprint build AiAssetMarketplaceEscrowJetton && blueprint build MentorStakingVault",
    "build": "npm run build:contracts",
    "test": "npm run build:contracts && jest --verbose",
    "test:backend": "npm run test:api --prefix backend",
    "test:backend:integration": "npm run test:api:integration --prefix backend",
    "test:miniapp:build": "npm run build --prefix miniapp",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "release": "blueprint pack && npm publish --access public",
    "dev": "concurrently -n backend,miniapp,admin -c blue,green,magenta \"npm run dev:backend\" \"npm run dev:miniapp\" \"npm run dev:admin\"",
    "dev:backend": "npm start --prefix backend",
    "dev:miniapp": "npm run dev --prefix miniapp",
    "dev:admin": "npm run dev --prefix admin-panel",
    "build:print-docs": "node scripts/build-print-docs.js",
    "predeploy:check": "node scripts/predeploy-check.js --prod",
    "migrate": "node scripts/run-migrations.js",
    "tunnel": "ngrok http 3000"
  },
  "dependencies": {
    "@jest/core": "^29.7.0",
    "@jest/environment": "^29.7.0",
    "@jest/expect": "^29.7.0",
    "@jest/fake-timers": "^29.7.0",
    "@jest/globals": "^27.5.1",
    "@jest/reporters": "^29.7.0",
    "@jest/test-result": "^29.7.0",
    "@jest/test-sequencer": "^29.7.0",
    "@jest/transform": "^29.7.0",
    "@ton/core": "^0.63.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-istanbul": "^7.0.1",
    "form-data": "^4.0.1",
    "glob": "^10.5.0",
    "jest-circus": "^25.2.3",
    "jest-config": "^29.7.0",
    "jest-runner": "^29.7.0",
    "jest-runtime": "^29.7.0",
    "jest-snapshot": "^29.7.0",
    "minimatch": "^10.2.1",
    "mongodb": "^7.1.0",
    "node-notifier": "^8.0.1",
    "qs": "^6.14.1",
    "rimraf": "^6.1.3",
    "test-exclude": "^7.0.1",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@tact-lang/compiler": "^1.6.7",
    "@ton-community/func-js": ">=0.10.0",
    "@ton/blueprint": "^0.36.1",
    "@ton/crypto": "^3.3.0",
    "@ton/sandbox": "^0.30.0",
    "@ton/test-utils": ">=0.11.0",
    "@ton/tolk-js": ">=1.0.0",
    "@ton/ton": ">=15.3.1 <16.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.17.2",
    "concurrently": "^9.1.0",
    "jest": "^19.0.2",
    "jest-resolve-dependencies": "^26.6.3",
    "ngrok": "^4.3.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}